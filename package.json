  "license": "CC0-1.0",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "autoprefixer": "^9.6.1",
    "bootstrap": "4.3.1",
    "css-loader": "^3.2.0",
    "electron": "^2.0.18",
    "file-loader": "^4.2.0",
    "jquery": "3.4.1",
    "popper.js": "1.15.0",
    "style-loader": "^1.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "xmlhttprequest-ts": "^1.0.1"
  }
